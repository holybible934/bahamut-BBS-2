<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ToolbarFloating"
    android:layout_width="80dp"
    android:layout_height="180dp"
    android:layout_marginEnd="0dp"
    android:orientation="vertical"
    android:background="@color/divider_color">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="1dp"
        style="@style/Toolbar">
        <Button android:id="@+id/ToolbarFloating_setting" android:text="@string/number_1" style="@style/ToolbarItem"/>
        <View style="@style/HorizontalDivider"/>
        <Button android:id="@+id/ToolbarFloating_1" android:text="@string/number_2" style="@style/ToolbarItem" android:autofillHints="" android:contentDescription="@string/no_use"/>
        <View style="@style/HorizontalDivider"/>
        <Button android:id="@+id/ToolbarFloating_2" android:text="@string/number_3" style="@style/ToolbarItem"/>
    </LinearLayout>
</LinearLayout>